package ver1;

/**
 * @author amarjeets
 *
 */
public class PerException extends Exception {

	public PerException() {
		super();
		// TODO Auto-generated constructor stub
	}

	public PerException(String arg0, Throwable arg1) {
		super(arg0, arg1);
		// TODO Auto-generated constructor stub
	}

	public PerException(String arg0) {
		super(arg0);
		// TODO Auto-generated constructor stub
	}

	public PerException(Throwable arg0) {
		super(arg0);
		// TODO Auto-generated constructor stub
	}
	
	
	

}
